{
    "name": "Product memory aide",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        
    },
    "action": {
      "default_popup": "hello.html"
    },
    "content_scripts": [{
      "matches": [
        "<all_urls>"
      ],    
      "js": ["content.js"]
    }],
    "permissions":[
        "scripting",
        "activeTab",
        "debugger"
    ]
  }
  